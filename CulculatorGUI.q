//20180410 v0.1面向对象初识
//20180417 v0.2制作计算器
//20180424 v0.3通过int()转换实现多位数的运算(运算更改为字符转换式)
//20180508 v0.4实现括号优先级(内含单次)运算
//20180515 v0.4.1继续实现
//(运算更新为类windows附件，连算前自动得出上一步结果)
//20180520 v0.4.2基本实现括号内容

Dimenv a, b, out,input//a,b为字符串
a = ""
b = ""
out = ""
input = true//为a赋值为true，为b赋值为false

DimEnv spcWrite,spcA,spcB,spcOut,spaceInput//括号字符串
spcA = ""
spcB = ""
spcOut = ""//运算结果
SpcReturn = ""//运算进程反馈
spcInput = true

spcWrite = false//括号运算时进行写入

Event Form1.out.Change
	
End Event

//Env
Event Form1.clean.Click
    input = true//为a赋值
    spcWrite = false
    a = ""
    b = ""
    spcA = ""
    spcB = ""
    form1.out.Text = "0"
End Event

Event Form1.back.Click//倒退一步
    //文本框
    form1.out.Text = Left(form1.out.Text,(Len(form1.out.Text) - 1))
    If Len(form1.out.Text) = 0 Then 
        form1.out.Text ="0"
    End If
    //运算符
    If input = false and b <> "" Then 
        b = left(b, (Len(b) - 1))
    ElseIf input = false and b = "" Then
        input = true
    End If
End Event

//括号
Event Form1.spcLeft.Click//左括号
    spcWrite = true//括号写入
    spcInput = true//A输入
    spcText = ""
    If form1.out.Text = "0" Then 
        form1.out.Text = "("	
    else
        form1.out.Text = form1.out.Text & "("
    End If
End Event

Event Form1.spcRight.Click//右括号 类似等于
    If spcWrite = true Then 
        spcWrite = false
        spcA = int(spcA)
        spcB = int(spcB)
        If culcAdd = true Then 
            spcOut = spcA + spcB
            culcAdd = false
        elseIf culcSub = true Then 
            spcOut = spcA - spcB
            culcSub = false
        elseIf culcMul = true Then 
            spcOut = spcA * spcB
            culcMul = false
        elseIf culcOff = true Then 
            spcOut = spcA / spcB
            culcOff = false
        End If
        If input = true Then 
            a = a & spcOut
        Else 
            b = b & spcOut
        End if
    End If
    If form1.out.Text = "0" Then 
        form1.out.Text = ")"	
    else
        form1.out.Text = form1.out.Text & ")"
    End If
    spcA = ""
    spcB = ""
End Event


