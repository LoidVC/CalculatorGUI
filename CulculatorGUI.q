//culculation
Event Form1.culcAdd.click
    If spcWrite = false then
        If a <> "" and b <> "" Then 
            a = int(a)
            b = int(b)
            If culcAdd = true Then 
                out = a + b
                culcAdd = false
            elseIf culcSub = true Then 
                out = a - b
                culcSub = false
            elseIf culcMul = true Then 
                out = a * b
                culcMul = false
            elseIf culcOff = true Then 
                out = a / b
                culcOff = false
            End If
            a = out
            b = ""
            form1.out.Text = out
        End If
        form1.out.Text = form1.out.Text & "+"	
        input = false
    End If
    If spcWrite = true then
        If spcA <> "" and spcB <> "" Then 
            spcA = int(spcA)
            spcB = int(spcB)
            If culcAdd = true Then 
                out = spcA + spcB
                culcAdd = false
            elseIf culcSub = true Then 
                out = spcA - spcB
                culcSub = false
            elseIf culcMul = true Then 
                out = spcA * spcB
                culcMul = false
            elseIf culcOff = true Then 
                out = spcA / spcB
                culcOff = false
            End If
            spcA = spcOut
            spcB = ""
            form1.out.Text = form1.out.Text & spcOut
        End If
        spcInput = false//B
        form1.out.Text = form1.out.Text & "+"
    End If
    culcAdd = true
End Event

