Event Form1.num1.click
    If  spcWrite = false then
        //交互反馈
        If form1.out.Text = "0" Then 
            form1.out.Text = "1"	
        else
            form1.out.Text = form1.out.Text & "1"
        End If
        //加数赋值
        If input = true Then 
            a = a & "1"
        Else 
            b = b & "1"
        End If
    End If
	
    //括号内
    If spcWrite = true Then 
        form1.out.Text = form1.out.Text & "1"
        //加数赋值
        If spcInput = true Then
            spcA = spcA & "1"
        Elseif spcInput = false then 
            spcB = spcB & "1"
        End If
    End If
End Event

